<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
   
    <title>Home</title>
    <link rel="stylesheet" href="/styles/styles.css" type="text/css">
</head>
<body>
   <h1>Cars</h1>
   <table>
       <thread>
           <tr><td>Model</td><td>License</td><td>Picture</td></tr>
       </thread>
       <tbody>
           <% cars.forEach( car => { %>
            <tr>
                <td> <%= car.model %> </td>
                <td> <%= car.license %> </td>
                <td>
                    <img src="/images/<%= car.picture %>" 
                    alt="A picture of model <%= car.model %>">
                </td>
            </tr>

           <% }); %>
       </tbody>
   </table>
   <a href="/">Home</a>
</body>
</html>