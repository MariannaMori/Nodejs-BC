<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Cats project</title>
    <link rel="stylesheet" href="/styles/styles.css" type="text/css">
</head>
<body>
    <main>
        <h2>All Cats</h2>
        <table>
            <thead>
                <tr>
                    <th>CatId</th>
                    <th>Name</th>
                    <th>Length</th>
                    <th>Breed</th>
                    <th>Year of Birth</th>
                </tr>
            </thead>
            <tbody>
                <% result.forEach(cat => { %>
                    <tr>
                        <td><%= cat.catId %></td>
                        <td><%= cat.name %></td>
                        <td><%= cat.length %></td>
                        <td><%= cat.breed %></td>
                        <td><%= cat.yearOfBirth %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
        <a href="/"><button>Menu</button></a>
    </main>
</body>
</html>